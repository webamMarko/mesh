{
    "name": "webam/mesh",
    "description": "Mesh module for PJU service autodiscovery",
    "autoload": {
        "psr-4": {
            "Pju\\Mesh\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marko Ambrožič",
            "email": "marko.ambrozic@pju.si"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "2.0.x-dev"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    },
    "repositories": [
        {
          "type": "composer",
          "url": "https://gitlab.com/api/v4/group/15872768/-/packages/composer/"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "pre-install-cmd": "validate_service_json.php"
    }
}
